rm pot_test_seded2.txt

cat pot_tested_values2.txt | sed 's/[^ ]* [^ ]* [^ ]*$//g' | sed 's/-0.000000\([ $]\)/0.000000\1/g' >> pot_tested_values_seded2.txt
